<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="cashbalance_wizard" model="ir.ui.view">
            <field name="name">cashbalance.wizard.form</field>
            <field name="model">cashbalance.wizard</field>
            <field name="arch" type="xml">
                <form string="Cash Balance">
                        <group>
                            <group col="2">
                                <field name="report_start" string="Date From"/>
                                 <field name="report_end" string="Date To"/>
                            </group>
                        </group>
                    <footer>
                        <button name="print_report" string="Print" type="object" class="oe_highlight"/>
                        <button string='Cancel' class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>