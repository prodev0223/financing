<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="expense_delivery_wizard_form" model="ir.ui.view">
            <field name="name">expense.delivery.wizard.form</field>
            <field name="model">hr.expense.picking.wizard</field>
            <field name="arch" type="xml">
                <form string="Create Delivery">
                    <group>
                        <field name="warehouse_id"/>
                        <field name="delivery_date"/>
                    </group>
                    <footer>
                        <button name="create_delivery" string="Create" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>