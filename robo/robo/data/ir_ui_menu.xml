            <field name="robo_front">True</field>
            <field name="robo_main_menu">True</field>
        </record>

        <record id="menu_pagalbininkas" model="ir.ui.menu">
            <field name="name">Robo Vadovas</field>
            <field name="web_icon">icon-briefcase,none,none</field>
