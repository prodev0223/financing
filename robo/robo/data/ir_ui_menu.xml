            <field name="sequence">10</field>
            <field name="parent_id" ref="robo.menu_start"/>
            <field name="robo_front">True</field>
        </record>

        <!--Back end-->
        <menuitem
                name="Kitos ataskaitos"
                id="meniu_kitos_ataskaitos"
