<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="needaction_accountant_wizard_view" model="ir.ui.view">
            <field name="name">needaction.accountant.wizard.view</field>
            <field name="model">needaction.accountant.wizard</field>
            <field name="robo_front">True</field>
            <field name="arch" type="xml">
                <form string="Duomenų importas">
                    <div>
                        <p>
                            Pagrįskite, kodėl šios sąnaudos <b>yra</b> priskiriamos reprezentacinėms sąnaudoms.
                        </p>
                    </div>
                    <group>
                        <field name="comment"/>
                    </group>
                    <footer>
                        <button string="Tvirtinu" name="post" type="object" class="btn-primary"/>
                        <button string="Atšaukti" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>