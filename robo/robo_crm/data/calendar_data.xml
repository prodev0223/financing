<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record id="calendar.subtype_invitation" model="mail.message.subtype">
            <field name="name">Pakvietimas</field>
            <field name="res_model">calendar.event</field>
            <field name="description" eval="False"/>
            <field name="default" eval="False"/>
        </record>
        <record id="calendar.alarm_notif_1" model="calendar.alarm">
            <field name="name">15 minučių</field>
            <field name="duration" eval="15" />
            <field name="interval">minutes</field>
            <field name="type">notification</field>
        </record>
        <record id="calendar.alarm_notif_2" model="calendar.alarm">
            <field name="name">30 minučių</field>
            <field name="duration" eval="30" />
            <field name="interval">minutes</field>
            <field name="type">notification</field>
        </record>
        <record id="calendar.alarm_notif_3" model="calendar.alarm">
            <field name="name">1 valanda</field>
            <field name="duration" eval="1" />
            <field name="interval">hours</field>
            <field name="type">notification</field>
        </record>
        <record id="calendar.alarm_notif_4" model="calendar.alarm">
            <field name="name">2 valandos</field>
            <field name="duration" eval="2" />
            <field name="interval">hours</field>
            <field name="type">notification</field>
        </record>
        <record id="calendar.alarm_notif_5" model="calendar.alarm">
            <field name="name">1 diena</field>
            <field name="duration" eval="1" />
            <field name="interval">days</field>
            <field name="type">notification</field>
        </record>
        <record id="calendar.alarm_mail_1" model="calendar.alarm">
            <field name="name">2~3 val. el. laiškas</field>
            <field name="duration" eval="3" />
            <field name="interval">hours</field>
            <field name="type">email</field>
        </record>
        <record id="calendar.alarm_mail_2" model="calendar.alarm">
            <field name="name">5~6 val. el. laiškas</field>
            <field name="duration" eval="6" />
            <field name="interval">hours</field>
            <field name="type">email</field>
        </record>

        <record id="calendar.categ_meet1" model="calendar.event.type">
            <field name="name">Susitikimas su klientu</field>
        </record>

        <record id="calendar.categ_meet2" model="calendar.event.type">
            <field name="name">Vidinis susitikimas</field>
        </record>

        <record id="calendar.categ_meet3" model="calendar.event.type">
            <field name="name">Neformalus susitikimas</field>
        </record>

        <record id="calendar.categ_meet4" model="calendar.event.type">
            <field name="name">Diskusija</field>
        </record>

        <record id="calendar.categ_meet5" model="calendar.event.type">
            <field name="name">Atsiliepimai</field>
        </record>


