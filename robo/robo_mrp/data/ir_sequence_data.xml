<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_stock_production_lot_number" model="ir.sequence">
            <field name="name">Production lot number sequence</field>
            <field name="code">stock.production.lot.number</field>
            <field name="prefix">LOT</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
        <record id="sequence_stock_production_serial_number" model="ir.sequence">
            <field name="name">Serial number sequence</field>
            <field name="code">stock.production.serial.number</field>
            <field name="prefix">SN#</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>