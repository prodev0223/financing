        <!--LT Bank Connectors-->
        <!--Very important to keep the names exactly like this (including upper-lower cases)-->
        <record id="enable_banking_connector_1_lt" model="enable.banking.connector">
            <field name="name">Citadele</field>
            <field name="api_code">cit_lt</field>
