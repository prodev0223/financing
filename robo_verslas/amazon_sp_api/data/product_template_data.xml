<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <!--Default Amazon products-->
        <record id="product_amazon_sp_gift_wraps" model="product.template">
            <field name="name">Amazon / Gift Wraps</field>
            <field name="default_code">AMZGFT</field>
            <field name="type">service</field>
            <field name="acc_product_type">service</field>
            <field name="categ_id" ref="amazon_sp_api.category_amazon_sp_special_services"/>
        </record>
        <record id="product_amazon_sp_shipping" model="product.template">
            <field name="name">Amazon / Shipping costs</field>
            <field name="default_code">AMZSHP</field>
            <field name="type">service</field>
            <field name="acc_product_type">service</field>
            <field name="categ_id" ref="amazon_sp_api.category_amazon_sp_special_services"/>
        </record>
        <record id="product_amazon_sp_promotion" model="product.template">
            <field name="name">Amazon / Promotion Discounts</field>
            <field name="default_code">AMZPRM</field>
            <field name="type">service</field>
            <field name="acc_product_type">service</field>
            <field name="categ_id" ref="amazon_sp_api.category_amazon_sp_special_services"/>
        </record>
        <record id="product_amazon_sp_fees" model="product.template">
            <field name="name">Amazon / Fees</field>
            <field name="default_code">AMZFEE</field>
            <field name="type">service</field>
            <field name="acc_product_type">service</field>
            <field name="categ_id" ref="amazon_sp_api.category_amazon_sp_special_services"/>
        </record>
    </data>
</odoo>