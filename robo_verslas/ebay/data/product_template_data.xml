<odoo>
    <data noupdate="1">
        <record id="ebay_product_main" model="product.template">
            <field name="name">eBay / Main</field>
            <field name="default_code">EBAY-MAIN</field>
            <field name="type">service</field>
            <field name="acc_product_type">service</field>
            <field name="categ_id" ref="ebay.category_ebay_goods"/>
        </record>
        <record id="ebay_product_shipping" model="product.template">
            <field name="name">eBay / Shipping and Handling</field>
            <field name="default_code">EBAY-SHIP</field>
            <field name="type">service</field>
            <field name="acc_product_type">service</field>
            <field name="categ_id" ref="ebay.category_ebay_services"/>
        </record>
        <record id="ebay_product_taxes" model="product.template">
            <field name="name">eBay / Collected Tax</field>
            <field name="default_code">EBAY-TAX</field>
            <field name="type">service</field>
            <field name="acc_product_type">service</field>
            <field name="categ_id" ref="ebay.category_ebay_goods"/>
        </record>
    </data>
</odoo>