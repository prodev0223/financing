<odoo>
    <data noupdate="1">
        <record id="robo_api_job_limit_amount" model="ir.config_parameter">
            <field name="key">robo_api_job_limit_amount</field>
            <field name="value">200</field>
            <field name="group_ids" eval="[(4, ref('base.group_system'))]"/>
        </record>

        <record id="robo_api_call_limit_per_hour" model="ir.config_parameter">
            <field name="key">robo_api_call_limit_per_hour</field>
            <field name="value">500</field>
            <field name="group_ids" eval="[(4, ref('base.group_system'))]"/>
        </record>

         <record id="robo_api_call_limit_per_minute" model="ir.config_parameter">
            <field name="key">robo_api_call_limit_per_minute</field>
            <field name="value">10</field>
            <field name="group_ids" eval="[(4, ref('base.group_system'))]"/>
        </record>

        <record id="robo_api_gl_entry_response_limit" model="ir.config_parameter">
            <field name="key">robo_api_gl_entry_response_limit</field>
            <field name="value">1000</field>
            <field name="group_ids" eval="[(4, ref('base.group_system'))]"/>
        </record>
    </data>
</odoo>